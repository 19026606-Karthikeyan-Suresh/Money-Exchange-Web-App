<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">Account List</h2>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="Account" asp-action="AddAccount" class="btn btn-primary">
                <i class="bi bi-plus-circle"></i> &nbsp; Create a new User
            </a>
        </div>
    </div>
    <br/><br/>

<table id="tblData" class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>#</th>
            <th>Email</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Phone Number</th>
            <th>Position</th>
            <th>Date Joined</th>
            <th>Actions</th>
        </tr>
    </thead>
</table>
</div>

@section Scripts{
    <script src="~/js/Account.js"></script>
}