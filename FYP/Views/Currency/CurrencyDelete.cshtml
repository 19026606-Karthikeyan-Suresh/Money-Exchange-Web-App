
    <div class="container">
        <form method="post" action="/Currency/CurrencyDeletePost">
            <div class="offset-sm-2 col-sm-10">
                <h3>Delete Currency</h3>
            </div>
            <div class="form-group row">
                <label for="stock_id" class="control-label col-sm-2">Stock id :</label>
                <div class="col-sm-4">
                    <input type="text" id="stock_id" name="stock_id" class="form-control" />
                </div>
                <div class="col-sm-1">
                    <input type="submit" value="Delete" class="btn btn-primary" />
                </div>
            </div>

            @if (ViewData["Message"] != null)
            {
                <div class="form-group row">
                    <div class="offset-sm-2 col-sm-5">
                        <div class="alert alert-@ViewData["MsgType"]">
                            @ViewData["Message"]
                        </div>
                    </div>
                </div>
            }
        </form>
    </div>
